  	}
  	
  	critique nameFromConcreteSyntaxIsDefined { //for each Existing Concrete Syntax a Name should be defined
      guard : self.satisfies("concreteSyntaxFromDSLIsDefined") 
      
      check {
	      	for(us in self.getUtilitySpecification){
